                                    <segue destination="6nc-gw-FP9" kind="presentation" modalTransitionStyle="partialCurl" id="4Sb-7w-PKv"/>
                                </connections>
                            </button>
                        </subviews>
                        <color key="backgroundColor" white="1" alpha="1" colorSpace="custom" customColorSpace="genericGamma22GrayColorSpace"/>
