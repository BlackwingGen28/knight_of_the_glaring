                <viewController id="3gK-KG-nnr" sceneMemberID="viewController">
                    <view key="view" contentMode="scaleToFill" id="SqT-OI-S8V">
                        <rect key="frame" x="0.0" y="0.0" width="375" height="667"/>
                        <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>
                        <subviews>
                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" fixedFrame="YES" textAlignment="natural" translatesAutoresizingMaskIntoConstraints="NO" id="mbB-BA-Og5">
                                <rect key="frame" x="71" y="37" width="288" height="522"/>
